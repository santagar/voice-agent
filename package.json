    "next": "16.0.3",
    "next-auth": "^4.24.13",
    "openai": "^6.9.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^9.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "prisma": "^6.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
